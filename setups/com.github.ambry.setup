<?xml version="1.0" encoding="UTF-8"?>
<setup:Project
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:git="http://www.eclipse.org/oomph/setup/git/1.0"
    xmlns:mylyn="http://www.eclipse.org/oomph/setup/mylyn/1.0"
    xmlns:predicates="http://www.eclipse.org/oomph/predicates/1.0"
    xmlns:projects="http://www.eclipse.org/oomph/setup/projects/1.0"
    xmlns:setup="http://www.eclipse.org/oomph/setup/1.0"
    xmlns:setup.targlets="http://www.eclipse.org/oomph/setup/targlets/1.0"
    xmlns:setup.workingsets="http://www.eclipse.org/oomph/setup/workingsets/1.0"
    xsi:schemaLocation="http://www.eclipse.org/oomph/setup/git/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Git.ecore http://www.eclipse.org/oomph/setup/mylyn/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Mylyn.ecore http://www.eclipse.org/oomph/predicates/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Predicates.ecore http://www.eclipse.org/oomph/setup/projects/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Projects.ecore http://www.eclipse.org/oomph/setup/targlets/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/SetupTarglets.ecore http://www.eclipse.org/oomph/setup/workingsets/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/SetupWorkingSets.ecore"
    name="com.github.ambry"
    label="Ambry">
  <setupTask
      xsi:type="setup.targlets:TargletTask">
    <targlet
        name="${scope.project.label}"
        activeRepositoryList="${eclipse.target.platform}"
        includeAllPlatforms="true">
      <annotation
          source="http:/www.eclipse.org/oomph/targlets/TargetDefinitionGenerator">
        <detail
            key="location">
          <value>${com.github.ambry.git.clone.location/releng/com.github.ambry.target/com.github.ambry.target.target}</value>
        </detail>
        <detail
            key="includeAllPlatforms">
          <value>true</value>
        </detail>
        <detail
            key="includeSource">
          <value>true</value>
        </detail>
        <detail
            key="generateVersions">
          <value>false</value>
        </detail>
        <detail
            key="generateImplicitUnits">
          <value>true</value>
        </detail>
        <detail
            key="includeMode">
          <value>slicer</value>
        </detail>
      </annotation>
      <requirement
          name="ch.qos.logback.slf4j"
          versionRange="[1.0.7,1.1.0)"/>
      <requirement
          name="ch.qos.logback.classic"
          versionRange="[1.0.7,1.1.0)"/>
      <requirement
          name="ch.qos.logback.core"
          versionRange="[1.0.7,1.1.0)"/>
      <requirement
          name="org.apache.felix.scr"/>
      <requirement
          name="org.osgi.service.component.annotations"/>
      <requirement
          name="org.eclipse.equinox.sdk.feature.group"/>
      <requirement
          name="org.eclipse.swt.dummyfragments.feature.group"/>
      <requirement
          name="org.slf4j.api"
          versionRange="[1.7.2,1.7.3)"/>
      <requirement
          name="org.slf4j.jcl"
          versionRange="[1.7.2,1.7.3)"/>
      <requirement
          name="org.slf4j.ext"
          versionRange="[1.7.2,1.7.3)"/>
      <requirement
          name="org.slf4j.jul"
          versionRange="[1.7.2,1.7.3)"/>
      <requirement
          name="org.slf4j.log4j"
          versionRange="[1.7.2,1.7.3)"/>
      <requirement
          name="org.apache.commons.codec"/>
      <requirement
          name="org.apache.commons.io"/>
      <requirement
          name="com.google.inject.multibindings"/>
      <requirement
          name="com.google.inject.assistedinject"/>
      <requirement
          name="org.hamcrest.core"
          versionRange="[1.1.0,1.2.0)"/>
      <requirement
          name="org.junit"/>
      <requirement
          name="org.mockito"
          versionRange="[1.9.5,1.10.0)"/>
      <requirement
          name="org.tukaani.xz"/>
      <requirement
          name="net.sf.trove4j"
          versionRange="[3.0.0,3.1.0)"/>
      <requirement
          name="com.google.guava"
          versionRange="[18.0.0,18.1.0)"/>
      <requirement
          name="javax.servlet"/>
      <requirement
          name="org.json"/>
      <requirement
          name="org.apache.mina.core"/>
      <requirement
          name="org.apache.sshd.core"/>
      <requirement
          name="bcprov"/>
      <requirement
          name="bcpkix"/>
      <requirement
          name="com.codahale.metrics.core"/>
      <requirement
          name="io.netty.buffer"/>
      <requirement
          name="io.netty.codec"/>
      <requirement
          name="io.netty.codec-http"/>
      <requirement
          name="io.netty.codec-socks"/>
      <requirement
          name="io.netty.common"/>
      <requirement
          name="io.netty.handler"/>
      <requirement
          name="io.netty.transport"/>
      <requirement
          name="*"/>
      <sourceLocator
          rootFolder="${com.github.ambry.git.clone.location}"
          locateNestedProjects="true"/>
      <repositoryList
          name="Neon">
        <repository
            url="http://download.eclipse.org/tools/orbit/downloads/drops/R20160520211859/repository"/>
        <repository
            url="http://download.eclipse.org/eclipse/updates/4.6"/>
        <repository
            url="http://10.72.103.217:8090/p2/ambry/"/>
        <repository
            url="http://10.72.103.217:8090/p2/netty/"/>
        <repository
            url="http://agentlab.ru/p2/orbit/missing-swt-fragments/"/>
      </repositoryList>
    </targlet>
  </setupTask>
  <setupTask
      xsi:type="setup.workingsets:WorkingSetTask"
      id="Ambry">
    <workingSet
        name="Ambry">
      <predicate
          xsi:type="predicates:LocationPredicate"
          pattern=".*/ambry/.*"/>
    </workingSet>
  </setupTask>
  <setupTask
      xsi:type="mylyn:MylynQueriesTask"
      connectorKind="github"
      repositoryURL="https://github.com/agentlab/JaxRSProviders"
      userID="${github.user.id}"
      password="${github.user.password}">
    <query summary="${scope.project.label} All Open Tasks">
      <attribute
          key="state"
          value="open::"/>
    </query>
    <query summary="${scope.project.label} My Open Tasks">
      <attribute
          key="state"
          value="open::"/>
      <attribute
          key="assignee"
          value="${github.user.id}"/>
    </query>
    <query summary="${scope.project.label} My Closed Tasks"
        url="">
      <attribute
          key="state"
          value="closed::"/>
      <attribute
          key="assignee"
          value="${github.user.id}"/>
    </query>
  </setupTask>
  <setupTask
      xsi:type="projects:ProjectsBuildTask"/>
  <stream name="mvn">
    <setupTask
        xsi:type="git:GitCloneTask"
        id="com.github.ambry.git.clone"
        remoteURI="agentlab/ambry">
      <annotation
          source="http://www.eclipse.org/oomph/setup/InducedChoices">
        <detail
            key="inherit">
          <value>github.remoteURIs</value>
        </detail>
        <detail
            key="label">
          <value>${scope.project.label} Github repository</value>
        </detail>
        <detail
            key="target">
          <value>remoteURI</value>
        </detail>
      </annotation>
      <description>${scope.project.label}</description>
    </setupTask>
  </stream>
  <logicalProjectContainer
      xsi:type="setup:ProjectCatalog"
      href="index:/org.eclipse.setup#//@projectCatalogs[name='ru.agentlab']"/>
  <description>IDE Setup for LinkedIn Ambry Project</description>
</setup:Project>
